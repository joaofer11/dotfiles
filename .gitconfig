[init]
	defaultBranch = main
[alias]
	# Log current branch
	l  = log --graph
	lo = log --graph --oneline

	# Status
	s  = status
	ss = status --short

	# Diff
	d  = diff
	dc = diff --cached

	# Add
	a  = add
	aa = add --all
	ae = add --edit
	u  = reset HEAD --

	# Commit
	c   = commit
	can = commit --amend --no-edit

